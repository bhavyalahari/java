import java.util.Scanner;					
class digit
{
	public static void main(String args[])
	{
		int r,num,sum=0;
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		num=n;
		while(n>0)
		{
			r=n%10;
			sum=sum+r;
			n=n/10;
			}
			System.out.println(num);
			if(num%sum==0)
			System.out.println("Harshed number");
			else
System.out.println("not a harshed number");
}
}
