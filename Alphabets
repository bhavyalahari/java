import java.util.Scanner;
	class Pattern3
{
	public static void main (String args[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("enter n value");
		int n=sc.nextInt();
		int i,j;
		char ch='A';
		for(i=1;i<=n;i++)
		{
			for(j=1;j<=i;j++)
			{
				System.out.print(ch+" ");
				ch+=1;
				}
				System.out.println();
				}
				}
				}
